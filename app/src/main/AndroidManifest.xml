<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dylanrodbar.reproductor">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MusicListActivity" />
        <activity android:name=".TopMusicListActivity" />
        <activity android:name=".RecentMusicListActivity" />
        <activity android:name=".RecommendedMusicListActivity" />
        <activity android:name=".MusicDetailActivity" />
        <activity
            android:name=".MainMediaPlayse"
            android:label="@string/title_activity_main_media_playse"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>